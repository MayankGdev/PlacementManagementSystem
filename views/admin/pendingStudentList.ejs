<%-include('../include/admin-header',{pageTitle:pageTitle,adminEmail:adminEmail})-%>
<center>
  <div class="content-wrapper" style="padding-left: 180px;">
    <div class="container-fluid">
        <div class="row">
          <div class="col-lg-12">
  <h1 class="my-2">Students list</h1>

<table class="table table-hover">
    <thead class="thead-dark">
      <tr>
        <th scope="col">Name</th>
        <th scope="col">Email</th>
        <th scope="col">Course</th>
        <th scope="col">Semester</th>
        <th scope="col">Action</th>
      </tr>
    </thead>
    <% if(Students.length!=0){Students.forEach(student=>{%>
    <tbody>
    
        <tr>
 
            <td><%=student.name%></td>
            <td><%=student.email%></td>
            <td><%=student.course%></td>
            <td><%=student.sem%></td>
           
         <td><a href="/pendingStudentview/<%=student._id%>"><i class="fa fa-eye" aria-hidden="true"></i></a>
            <a href="/pendingStudent/accept/<%=student._id%>"><i class="fa fa-check" aria-hidden="true"></i></a>
            <a href="/pendingStudent/delete/<%=student._id%>"><i class="fa fa-trash-o" aria-hidden="true"></i></a>
        </td>
          </tr>
    </tbody>
    <%});}else{%>
<h4>no records found</h4>
        <%}%>
        
  </table>
          </div>
        </div>
    </div></div></center>

<%-include('../include/admin-footer')-%>