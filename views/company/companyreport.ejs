<%-include('../include/company-header',{pageTitle:pageTitle,compEmail : compEmail})-%>
<center>
  <div class="content-wrapper" style="padding-left: 180px;">
    <div class="container-fluid">
        <div class="row">
          <div class="col-lg-12">
          
      
 

    
    <div class="conatiner">
        <h3>Job list</h3>
        <table class="table table-hover">
            <thead class="thead-dark">
              <tr>
                <th scope="col">Job title</th>
               
                <th scope="col">type</th>
               
                <th scope="col">Interview Date</th>
                <th scope="col">Action</th>
              </tr>
            </thead>
            <% if(company.length!=0){company.forEach(company=>{%>
            <tbody>
            
                <tr>
         
                    <td><%=company.title%></td>
                   
                    <td><%=company.type%></td>
                 
                    <td><%=company.interviewDate%></td>
                 <td><a href="/companyjobs/<%=company._id%>"><i class="fa fa-eye" aria-hidden="true"></i></a>
                </td>
                  </tr>
            </tbody>
            <%});}else{%>
          <h4>no records found</h4>
                <%}%>
                
          </table>
    </div>
          </div>
        </div>
      </div>
    </div>
</div>
</center>
<%-include('../include/company-footer')-%>